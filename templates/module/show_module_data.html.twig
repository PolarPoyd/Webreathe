{% extends 'base.html.twig' %}

{% block title %}Détails des données
{% endblock %}

{% block javascript %}
<script src="https://cdn.jsdelivr.net/npm/axios@0.21.1/dist/axios.min.js"></script>

{% endblock %}

{% block body %}

		<h1 class="text-center mb-5 mt-5">Données de votre
			{{ module.name }}</h1>
	<div class="container text-center">

		<div class="row">
			<div class="col-md-6">
				<h2>Température</h2>
				<p>5 dernières entrées</p>
				{{ include("graph/graph_energy.html.twig") }}
			</div>
			<div class="col-md-6">
				<h2>Consomation d'énergie</h2>
				<p>5 dernières entrées</p>
				{{ include("graph/graph_temperature.html.twig") }}
			</div>
		</div>

		<div class="row mt-4">
			<div class="col-md-6">
				<h2>Débit</h2>
				<p>5 dernières entrées</p>
				{{ include("graph/graph_flow.html.twig") }}
			</div>
			<div class="col-md-6">
				<h2>Vitesse</h2>
				<p>5 dernières entrées</p>
				{{ include("graph/graph_speed.html.twig") }}
			</div>
		</div>
		<div class="text-center mt-4 mb-4">
		
		<a href="{{ path('app_home') }}" class="btn btn-primary mt-5">Retour</a>
		</div>
	<div style="height: 200px;"></div>

	</div>

{% endblock %}
